import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int count = 0;
        if (num == 0) {
            count = 1;
        } else {
            while (num != 0) {
                num = num / 10;  // Remove the last digit
                count++;
            }
        }

        System.out.println(count);
    }
}

