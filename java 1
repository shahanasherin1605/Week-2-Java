import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner kbd=new Scanner(System.in);
		int n=kbd.nextInt();
		int dig=0;
		while(n>0){
		    dig=n%10;
	        System.out.println(dig+" ");
		    n=n/10;
		}
	}
}

Input:1234
Output:1 2 3 4

Input:1001
Output:1 0 0 1
