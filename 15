import java.util.Scanner;

public class DigitFrequency {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String num = sc.next();          // Read input as String to preserve zeros

        int[] freq = new int[10];        // Array to store counts for digits 0–9

        // Count each digit
        for (char ch : num.toCharArray()) {
            freq[ch - '0']++;
        }

        // Print digits that appear, in ascending order
        for (int i = 0; i <= 9; i++) {
            if (freq[i] > 0) {
                System.out.println(i + "→" + freq[i]);
            }
        }
    }
}
